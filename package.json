		"build:client": "cross-env NODE_ENV=production webpack -p --mode production --config build/webpack.client.config.js --progress --hide-modules",
		"build:server": "cross-env NODE_ENV=production webpack -p --mode production --config build/webpack.server.config.js --progress --hide-modules",
