		"test": "cross-env NODE_ENV=development webpack --config build/setup-dev-server.js --progress --hide-modules --display=minimal --color --watch",
		"dev:server": "cross-env NODE_ENV=development webpack -p --mode development --config ./build/webpack.server.config.js --display=minimal --color",
		"start": "cross-env NODE_ENV=production node server",
