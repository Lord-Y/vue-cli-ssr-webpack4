{
  "presets": [
    [
      "es2015"
    ],
    [
      "env",
      {
        "modules": false
      }
    ],
    "stage-0",
    "stage-2",
    "stage-3"
  ],
  "plugins": [
    "syntax-dynamic-import",
    [
      "transform-runtime",
      {
        "polyfill": true,
        "regenerator": true
      }
    ]
  ]
}
